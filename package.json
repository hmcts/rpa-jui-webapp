{
  "name": "jui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run build-universal",
    "build-universal": "npm run build:client-and-server-bundles",
    "start": "node server.js",
    "start-dev": "ng serve",
    "start-dev-proxy": "ng serve --proxy-config proxy.config.json",
    "start-dev-docker-proxy": "export JUI_ENV=docker; ng serve --proxy-config proxy.config.json",
    "start-dev-node": "export JUI_ENV=local; node dev-server.js",
    "start-dev-docker-node": "export JUI_ENV=docker; node dev-server.js",
    "e2e": "protractor ./test/e2e/config/conf.js",
    "dev": "yarn build-universal && concurrently \"yarn dev-frontend\" \"yarn dev-backend\"",
    "dev-backend": "nodemon server.js --watch api --watch src --watch middleware --watch lib --watch proxies --watch config --delay 3000ms -e js,ts,html,scss",
    "dev-frontend": "ng-udkc",
    "build:client-and-server-bundles": "ng build --prod && ng run jui:server",
    "lint": "(ng lint jui --force && eslint . --ignore-path .gitignore) || :",
    "test": "jasmine --config=spec/support/jasmine.json && ng test",
    "test:nsp": "echo \"Security testing\"",
    "test:a11y": "echo \"Testing ally\"",
    "test:coverage": "ng test && ng test --main ./src/test-coverage.ts --code-coverage",
    "test-all": "echo 'test all not implemented'",
    "test:nsp-warn": "echo \"Test nsp warn\"",
    "test:snyk": "echo \"Test snyk\"",
    "test:smoke": "echo 'smoke test not implemented'",
    "test:functional": "echo 'functional test not implemented'",
    "health-check": "echo 'healthcheck not implemented'",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:codacy-upload": "echo \"Test codacity\"",
    "test:codecov-upload": "\"echo \"Test codacity\"",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
